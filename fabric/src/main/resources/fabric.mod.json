{
  "schemaVersion": 1,
  "id": "examplemod",
  "version": "${version}",
  "name": "Example Mod",
  "description": "A Minecraft Mod.",
  "authors": [
    "Me!"
  ],
  "contact": {
    "homepage": "https://github.com/"
  },
  "license": "CC0-1.0",
  "icon": "assets/examplemod/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "com.example.examplemod.fabric.ExampleModFabric"
    ],
    "client": [
      "com.example.examplemod.fabric.client.ExampleModClientFabric"
    ],
    "fabric-datagen": [
      "com.example.examplemod.fabric.datagen.ExampleModDataGenFabric"
    ],
    "modmenu": [
      "com.example.examplemod.fabric.impl.ModMenuImpl"
    ]
  },
  "mixins": [
    "examplemod.mixins.json",
    "examplemod-common.mixins.json"
  ],
  "depends": {
    "minecraft": ">=1.19 <=1.19.2",
    "fabric": "*",
    "architectury": "*"
  },
  "suggests": {
    "modmenu": "*"
  }
}